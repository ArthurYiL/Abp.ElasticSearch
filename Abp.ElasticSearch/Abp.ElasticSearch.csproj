<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.2.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>LiangShiWei</Authors>
    <Company />
    <Description>AbpElasticSearch扩展包，默认集成elasticsearch，使用时需要在启动时使用ElasticSearchConfiguration类进行配置连接连接字符串还有授权</Description>
    <RepositoryUrl>https://github.com/ShiWei-L/Abp.ElasticSearch</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Abp" Version="3.8.2" />
    <PackageReference Include="NEST" Version="6.2.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
